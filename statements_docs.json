    "/V1/payments": {
      "get": {
        "summary": "all payments for all publishers",
        "description": "all payments for all publishers",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "query",
            "name": "token",
            "description": "The authentication token for a user.",
            "type": "string",
            "required": true
          },
          {
            "in": "query",
            "name": "order",
            "description": "Specify ordering for attributes.",
            "type": "string",
            "default": "CHECK_CUT_DATE desc",
            "required": false
          },
          {
            "in": "query",
            "name": "page",
            "description": "Page offset to fetch.",
            "type": "integer",
            "format": "int32",
            "default": 1,
            "required": false
          },
          {
            "in": "query",
            "name": "per_page",
            "description": "Number of results to return per page.",
            "type": "integer",
            "format": "int32",
            "default": 10,
            "required": false
          },
          {
            "in": "query",
            "name": "offset",
            "description": "Pad a number of results.",
            "type": "integer",
            "format": "int32",
            "default": 0,
            "required": false
          }
        ],
        "responses": {
          "200": {
            "description": "all payments for all publishers"
          }
        },
        "tags": [
          "payments"
        ],
        "operationId": "getV1Payments"
      }
    },
